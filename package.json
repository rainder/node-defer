{
  "name": "@rainder/defer",
  "version": "0.2.1",
  "description": "Simple promise wrapper",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "nyc mocha --require ts-node/register 'lib/*.spec.ts'",
    "build": "tsc -d",
    "prepublish": "npm run build"
  },
  "keywords": [
    "defer",
    "promise"
  ],
  "author": "Andrius Skerla",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainder/node-defer.git"
  },
  "bugs": {
    "url": "https://github.com/rainder/node-defer/issues"
  },
  "homepage": "https://github.com/rainder/node-defer#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "include": [
      "lib"
    ],
    "exclude": [
      "lib/*.spec.ts",
      "lib/*.d.ts"
    ]
  }
}
